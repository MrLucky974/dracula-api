{
  "schemaVersion": 1,
  "id": "dracula_api",
  "version": "${version}",
  "name": "Dracula API",
  "description": "A collection of shared code across my Minecraft mods",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/dracula_api/icon.png",
  "accessWidener": "dracula_api.classtweaker",
  "environment": "*",
  "entrypoints": {
    "client": [
      "io.github.mrlucky974.dracula_api.client.DraculaAPIClient"
    ],
    "main": [
      "io.github.mrlucky974.dracula_api.DraculaAPI"
    ],
    "mm:early_risers": [
      "io.github.mrlucky974.dracula_api.EarlyRiser"
    ],
    "dracula_api:crossbow_charge_types": [
      "io.github.mrlucky974.dracula_api.test.item.ExampleCrossbowChargeTypes"
    ]
  },
  "mixins": [
    "dracula_api.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "mm": ">=${fabric_asm_version}"
  }
}
